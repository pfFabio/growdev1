import math

while True:
    a = float(input("me diga o valor de a\n"))
    b = float(input("me diga o valor de b\n"))
    c = float(input("me diga o valor de c\n"))
    delta = math.sqrt(b**2 - 4 * a * c)
    raiz1 = (-b+delta)/(2*a)
    raiz2 = (-b-delta)/(2*a)
    print(f"a ra√≠zes foram {raiz1} e {raiz2}") 
