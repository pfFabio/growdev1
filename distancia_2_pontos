import math

def calcula(primeiro, segundo):
    horizontal = primeiro["x"] - segundo["x"]
    vertical = primeiro["y"] - segundo["y"]
    distancia = math.sqrt((pow(horizontal, 2) + pow(vertical, 2)))
    print(f"a distancia Ã© {distancia}")

primeiro = {"x" : 0, "y" : 0}
segundo = {"x" : 4, "y" : 3}
calcula(primeiro, segundo)
